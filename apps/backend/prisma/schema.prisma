generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id         Int      @id @default(autoincrement())
  name       String
  uuid       String   @unique
  price      Int
  created_at DateTime @default(now())
  updated_at DateTime @default(now())

  @@map("books")
}
